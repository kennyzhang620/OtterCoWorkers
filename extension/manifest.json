{
    "manifest_version": 3,
    "name": "Otter Co-Worker",
    "version": "1.0",
    "description": "Displays stats of tab domains.",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js": ["animator.js", "sparky_animations.js", "stormy_animations.js", "animator_controller.js","contentTabTimer.js","popup.js","persitentTabTimer.js", "content.js"]
        } 
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "icons": {
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
  },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {"resources": [ "*" ],
      "extension_ids": ["*"],
      "matches": [ "*://*/*" ]}
    ],
    "options_page": "stats.html"
  }
  