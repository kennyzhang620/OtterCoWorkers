{
    "name": "bear",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js": ["content.js"]
        } 
    ],
    "action":{
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {"resources": [ "*" ],
        "extension_ids": ["*"],
        "matches": [ "*://*/*" ]}
      ],
    "permissions": ["activeTab"]
}