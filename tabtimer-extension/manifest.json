{
    "manifest_version": 3,
    "name": "Show Tab Title & Chrome Time",
    "version": "1.1",
    "description": "Displays website domain and estimated Chrome window open time",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js" // Specify the background script
    }
  }
  