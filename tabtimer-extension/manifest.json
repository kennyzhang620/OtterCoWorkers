{
    "manifest_version": 3,
    "name": "Tab Domains Viewer",
    "version": "1.0",
    "description": "Displays stats of tab domains.",
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js": ["contentTabTimer.js","popup.js","persitentTabTimer.js"]
        } 
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "stats.html"
  }
  