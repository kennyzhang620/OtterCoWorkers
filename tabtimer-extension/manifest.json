{
    "manifest_version": 3,
    "name": "Tab Domains Viewer",
    "version": "1.0",
    "description": "Displays stats of tab domains.",
    "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "stats.html"
  }
  